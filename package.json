{
  "name": "playwright-ts",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "encrypt-stg-env": "./node_modules/.bin/dotenvenc -e -i ./utils/env/.env.stg -o ./utils/env/.stg.enc",
    "encrypt-uat-env": "./node_modules/.bin/dotenvenc -e -i ./utils/env/.env.uat -o ./utils/env/.uat.enc",
    "encrypt-prod-env": "./node_modules/.bin/dotenvenc -e -i ./utils/env/.env.prod -o ./utils/env/.prod.enc",
    "encrypt-env": "npm run encrypt-stg-env && npm run encrypt-uat-env && npm run encrypt-prod-env",
    "test-stg": "npx dotenv -v DOTENVENC_KEY=${{env.DOTENVENC_KEY}} npx dotenv -v ENV=stg npx playwright test",
    "test-uat": "npx dotenv -v ENV=uat npx playwright test",
    "test-prod": "npx dotenv -v ENV=prod npx playwright test"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@playwright/test": "^1.35.1",
    "@types/fs-extra": "^11.0.1",
    "@types/uuid": "^9.0.2",
    "monocart-reporter": "^1.7.3"
  },
  "dependencies": {
    "@tka85/dotenvenc": "^4.0.2",
    "@types/node": "^20.3.1",
    "dotenv": "^16.3.0",
    "dotenv-cli": "^7.2.1",
    "fs-extra": "^11.1.1",
    "uuid": "^9.0.0"
  }
}
